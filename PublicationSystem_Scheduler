CREATE NONEDITIONABLE  VIEW titleview SHARING = NONE ( BKNM,AUTNO,MSRP,PBLRNO ) 
	DEFAULT COLLATION USING_NLS_COMP
	BEQUEATH DEFINER
	 AS  SELECT BK.BKNM,AUT.AUTNO,BK.MSRP,PBLR.PBLRNO
		FROM AUT ,BK ,PBLR ;

CREATE NONEDITIONABLE  VIEW Order_View SHARING = NONE ( STRNM,ORDNO,ORDDT,BKNM,ORDQNT,ORDDSCNTAMT,ORDAMT ) 
	DEFAULT COLLATION USING_NLS_COMP
	BEQUEATH DEFINER
	 AS  SELECT STR.STRNM,ORD.ORDNO,ORD.ORDDT,BK.BKNM,ORDDTL.ORDQNT,ORDDTL.ORDDSCNTAMT,ORDDTL.ORDAMT
		FROM ORDDTL ,BK ,ORD ,STR ;

CREATE NONEDITIONABLE  VIEW Publisher_View SHARING = NONE ( EMPNM,PBLRNM,BKNM,YTDNTSLQNT ) 
	DEFAULT COLLATION USING_NLS_COMP
	BEQUEATH DEFINER
	 AS  SELECT EMP.EMPNM,PBLR.PBLRNM,BK.BKNM,YTDNTSL.YTDNTSLQNT
		FROM PBLR ,BK ,YTDNTSL ,EMP ;

CREATE NONEDITIONABLE  VIEW Payment_View SHARING = NONE ( CRDTCARDNO,CRDTCARDAMT,MNORDNO,MNORDAMT,CHCKNO,CHCKAMT,CUSTNM,ORDNO,ORDDT ) 
	DEFAULT COLLATION USING_NLS_COMP
	BEQUEATH DEFINER
	 AS  SELECT CRDTCARD.CRDTCARDNO,CRDTCARD.CRDTCARDAMT,MNORD.MNORDNO,MNORD.MNORDAMT,CHCK.CHCKNO,CHCK.CHCKAMT,CUST.CUSTNM,ORD.ORDNO,ORD.ORDDT
		FROM MNORD ,CRDTCARD ,CHCK ,CUST ,ORD ;

