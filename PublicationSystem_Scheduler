
CREATE TABLE AUT
SHARING =  NONE 
(
	AUTNO                VARCHAR2(20 BYTE) COLLATE USING_NLS_COMP VISIBLE  CONSTRAINT SYS_C009057 NOT NULL ,
	AUTNM                VARCHAR2(40 BYTE) COLLATE USING_NLS_COMP VISIBLE  CONSTRAINT SYS_C009058 NOT NULL ,
	AUTTELNO             VARCHAR2(11 BYTE) COLLATE USING_NLS_COMP VISIBLE  CONSTRAINT SYS_C009059 NOT NULL ,
	AUTBADDR             VARCHAR2(200 BYTE) COLLATE USING_NLS_COMP VISIBLE  CONSTRAINT SYS_C009060 NOT NULL ,
	AUTDADDR             VARCHAR2(200 BYTE) COLLATE USING_NLS_COMP VISIBLE  CONSTRAINT SYS_C009061 NOT NULL ,
	AUTZIP               CHAR(5 BYTE) COLLATE USING_NLS_COMP VISIBLE  CONSTRAINT SYS_C009062 NOT NULL ,
	CTRTFILE             BLOB  VISIBLE  CONSTRAINT SYS_C009063 NOT NULL 
)
	DEFAULT COLLATION USING_NLS_COMP;

CREATE UNIQUE INDEX AUT_PK ON AUT
(AUTNO   ASC)
	USABLE
	IMMEDIATE INVALIDATION;

ALTER TABLE AUT
	ADD CONSTRAINT  AUT_PK PRIMARY KEY (AUTNO);

CREATE UNIQUE INDEX AUT_UK1 ON AUT
(AUTTELNO   ASC,AUTBADDR   ASC,AUTDADDR   ASC,AUTZIP   ASC)
	USABLE
	IMMEDIATE INVALIDATION;

ALTER TABLE AUT
ADD 
CONSTRAINT  AUT_UK1 UNIQUE (AUTTELNO,AUTBADDR,AUTDADDR,AUTZIP);

CREATE UNIQUE INDEX AUT_UK2 ON AUT
(AUTBADDR   ASC,AUTDADDR   ASC,AUTZIP   ASC)
	USABLE;

